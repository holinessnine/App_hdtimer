        android:id="@+id/bt_3_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="100"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#CFD2E3"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box3"
        app:layout_constraintEnd_toStartOf="@+id/bt_3_plus"
        app:layout_constraintStart_toEndOf="@+id/bt_3_min"
        app:layout_constraintTop_toTopOf="@+id/timer_box3" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_3_plus"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="#F4EDED"
        android:text="+"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box3"
        app:layout_constraintEnd_toEndOf="@+id/timer_box3"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="@+id/timer_box3"
        app:layout_constraintTop_toTopOf="@+id/timer_box3" />

    <TextView
        android:id="@+id/timer_box4"
        android:layout_width="310dp"
        android:layout_height="50dp"
        android:background="@drawable/transparent_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49" />

    <TextView
        android:id="@+id/timer_gameTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="게임 시간(분)"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#91C3DD"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box4"
        app:layout_constraintEnd_toEndOf="@+id/timer_box4"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/timer_box4"
        app:layout_constraintTop_toTopOf="@+id/timer_box4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_4_min"
