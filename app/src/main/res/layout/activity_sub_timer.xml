<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#302E33"
    tools:context=".SubActivity_timer">


    <TextView
        android:id="@+id/timer_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare"
        android:text="게임 구성하기"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FAF3F3"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/timer_box1"
        android:layout_width="310dp"
        android:layout_height="50dp"
        android:background="@drawable/transparent_button"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19" />

    <TextView
        android:id="@+id/timer_people"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="참가 인원(명)"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#91C3DD"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box1"
        app:layout_constraintEnd_toEndOf="@+id/timer_box1"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/timer_box1"
        app:layout_constraintTop_toTopOf="@+id/timer_box1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_1_min"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="#F4EDED"
        android:text="-"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box1"
        app:layout_constraintEnd_toEndOf="@+id/timer_box1"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="@+id/timer_box1"
        app:layout_constraintTop_toTopOf="@+id/timer_box1" />

    <TextView
        android:id="@+id/bt_1_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="2"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#CFD2E3"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box1"
        app:layout_constraintEnd_toStartOf="@+id/bt_1_plus"
        app:layout_constraintStart_toEndOf="@+id/bt_1_min"
        app:layout_constraintTop_toTopOf="@+id/timer_box1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_1_plus"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="#F4EDED"
        android:text="+"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box1"
        app:layout_constraintEnd_toEndOf="@+id/timer_box1"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="@+id/timer_box1"
