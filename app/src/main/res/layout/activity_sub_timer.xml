        app:layout_constraintTop_toTopOf="@+id/timer_box4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_4_plus"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="#F4EDED"
        android:text="+"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box4"
        app:layout_constraintEnd_toEndOf="@+id/timer_box4"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="@+id/timer_box4"
        app:layout_constraintTop_toTopOf="@+id/timer_box4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/timer_bt_start"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="274dp"
        android:layout_height="58dp"
        android:background="@drawable/transparent_button"
        android:clickable="true"
        android:cursorVisible="false"
        android:fontFamily="@font/nanumsquare"
        android:text="게임 시작"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.696" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/appCompatImageView"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@drawable/hd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.88" />

    <TextView
        android:id="@+id/timer_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare"
        android:text="홀덤 타이머"
        android:textAlignment="center"
        android:textColor="#CCDAEA"
        android:textColorHighlight="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appCompatImageView"
        app:layout_constraintVertical_bias="0.120000005" />

