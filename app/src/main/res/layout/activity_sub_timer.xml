        android:background="#F4EDED"
        android:text="-"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box2"
        app:layout_constraintEnd_toEndOf="@+id/timer_box2"
        app:layout_constraintHorizontal_bias="0.65"
        app:layout_constraintStart_toStartOf="@+id/timer_box2"
        app:layout_constraintTop_toTopOf="@+id/timer_box2" />

    <TextView
        android:id="@+id/bt_2_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="1000"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#CFD2E3"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box2"
        app:layout_constraintEnd_toStartOf="@+id/bt_2_plus"
        app:layout_constraintStart_toEndOf="@+id/bt_2_min"
        app:layout_constraintTop_toTopOf="@+id/timer_box2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_2_plus"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="#F4EDED"
        android:text="+"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box2"
        app:layout_constraintEnd_toEndOf="@+id/timer_box2"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="@+id/timer_box2"
        app:layout_constraintTop_toTopOf="@+id/timer_box2" />

    <TextView
        android:id="@+id/timer_box3"
        android:layout_width="310dp"
        android:layout_height="50dp"
        android:background="@drawable/transparent_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39" />

    <TextView
        android:id="@+id/timer_startBlind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquare_0"
        android:text="시작 블라인드(스몰)"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#91C3DD"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/timer_box3"
        app:layout_constraintEnd_toEndOf="@+id/timer_box3"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/timer_box3"
        app:layout_constraintTop_toTopOf="@+id/timer_box3" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bt_3_min"
